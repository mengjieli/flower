<f:Group class="AddAnimationEffect" percentWidth="100" percentHeight="100"
         xmlns:f="flower" xmlns:s="modules/software/module.json"
         xmlns:e="modules/gameEditor/module.json">
    <f:script src="./AddAnimationEffect.script"/>
    <f:Group horizontalCenter="0" y="0">
        <f:Label horizontalCenter="-55" width="60" text="tag" verticalCenter="0"/>
        <s:Input id="tagInput" horizontalCenter="30" width="100" verticalCenter="0"/>
    </f:Group>
    <f:Group horizontalCenter="0" y="30">
        <f:Label horizontalCenter="-55" width="60" text="类型" verticalCenter="0"/>
        <s:ComboBox id="typeComboBox" horizontalCenter="30" width="100" verticalCenter="0"/>
    </f:Group>
    <f:Group horizontalCenter="0" y="60">
        <f:Label horizontalCenter="-55" width="60" text="资源" verticalCenter="0"/>
        <s:ComboBox id="urlComboBox" horizontalCenter="30" width="100" verticalCenter="0"/>
    </f:Group>
    <f:Group horizontalCenter="0" y="90">
        <f:Label horizontalCenter="-55" width="60" text="层次" verticalCenter="0"/>
        <s:ComboBox id="layerComboBox" horizontalCenter="30" width="100" verticalCenter="0"/>
    </f:Group>
    <f:Group horizontalCenter="0" y="120">
        <f:Label horizontalCenter="-55" width="60" text="缓动类型" verticalCenter="0"/>
        <s:ComboBox id="easeComboBox" horizontalCenter="30" width="100" verticalCenter="0"/>
    </f:Group>
    <f:Group horizontalCenter="0" y="150">
        <f:Label horizontalCenter="-55" width="60" text="缓动时间" verticalCenter="0"/>
        <s:Input id="easeTimeInput" horizontalCenter="30" width="100" verticalCenter="0"/>
    </f:Group>
    <f:Group horizontalCenter="0" y="180">
        <f:Label horizontalCenter="-55" width="60" text="像素坐标X" verticalCenter="0"/>
        <s:Input id="xInput" horizontalCenter="30" width="100" verticalCenter="0"/>
    </f:Group>
    <f:Group horizontalCenter="0" y="210">
        <f:Label horizontalCenter="-55" width="60" text="像素坐标Y" verticalCenter="0"/>
        <s:Input id="yInput" horizontalCenter="30" width="100" verticalCenter="0"/>
    </f:Group>
</f:Group>