<f:Group class="DamageRangeEffect" percentWidth="100" percentHeight="100"
         xmlns:f="flower" xmlns:s="modules/software/module.json"
         xmlns:e="modules/gameEditor/module.json" >
    <f:script src="./DamageRangeEffect.script"/>
    <f:Group horizontalCenter="0">
        <f:Label horizontalCenter="-55" width="60" text="陷阱id" verticalCenter="0"/>
        <s:Input id="idInput" horizontalCenter="30" width="100" verticalCenter="0"/>
    </f:Group>
    <f:Group horizontalCenter="0" y="30">
        <f:Label horizontalCenter="-55" width="60" text="格子坐标x" verticalCenter="0"/>
        <s:Input id="xInput" horizontalCenter="30" width="100" verticalCenter="0"/>
    </f:Group>
    <f:Group horizontalCenter="0" y="60">
        <f:Label horizontalCenter="-55" width="60" text="格子坐标y" verticalCenter="0"/>
        <s:Input id="yInput" horizontalCenter="30" width="100" verticalCenter="0"/>
    </f:Group>
    <f:Group horizontalCenter="0" y="90">
        <f:Label horizontalCenter="-55" width="60" text="模拟范围" verticalCenter="0"/>
        <s:Input id="rangeInput" horizontalCenter="30" width="100" verticalCenter="0"/>
    </f:Group>
    <f:Group horizontalCenter="0" y="120">
        <f:Label horizontalCenter="-55" width="60" text="模拟伤害值" verticalCenter="0"/>
        <s:Input id="damageInput" horizontalCenter="30" width="100" verticalCenter="0"/>
    </f:Group>
</f:Group>