<f:MaskUI class="Tab" width="400" height="300" xmlns:f="flower" xmlns:s="modules/software/module.json">
    <f:script src="./Tab.script"/>
    <f:Rect percentWidth="100" percentHeight="100" fillColor="0xffffff" lineColor="0x333333" lineWidth="1"/>
    <f:Group percentWidth="100" height="25">
        <f:Rect percentWidth="100" percentHeight="100" fillColor="0xcccccc"
                  lineColor="0x333333"
                  lineWidth="1"/>
        <s:TabBar id="tabBar" dataProvider="{this.viewStack}"/>
    </f:Group>
    <f:MaskUI percentWidth="100" top="25" bottom="0">
        <f:ViewStack id="viewStack" percentWidth="100" percentHeight="100">
        </f:ViewStack>
    </f:MaskUI>
</f:MaskUI>